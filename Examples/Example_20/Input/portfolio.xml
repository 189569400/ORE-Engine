  <!-- 3-6% tranche of Index CDS, e.g. iTraxx, represented as CDO -->
  <Trade id="CDS_INDEX_TRANCHE">
    <TradeType>SyntheticCDO</TradeType>
    <Envelope>
      <CounterParty>CPTY_A</CounterParty>
      <NettingSetId>CPTY_A</NettingSetId>
      <AdditionalFields/>
    </Envelope>
    <CDOData>
      <ProtectionStart>20170410</ProtectionStart>
      <UpfrontDate/>
      <UpfrontFee>0.0324</UpfrontFee>
      <AttachmentPoint>0.03</AttachmentPoint>
      <DetachmentPoint>0.06</DetachmentPoint>
      <!-- Premium leg -->
      <LegData>
        <LegType>Fixed</LegType>
        <Payer>true</Payer>
        <!-- protection buyer pays premium -->
        <Currency>USD</Currency>
        <Notionals>
          <Notional>10000000.000000</Notional>
        </Notionals>
        <DayCounter>A360</DayCounter>
        <PaymentConvention>F</PaymentConvention>
        <FixedLegData>
