        </FixedLegData>
        <ScheduleData>
          <Rules>
            <StartDate>20160203</StartDate>
            <EndDate>20260203</EndDate>
            <Tenor>3M</Tenor>
            <Calendar>TARGET</Calendar>
            <Convention>F</Convention>
            <TermConvention>F</TermConvention>
            <Rule>CDS</Rule>
            <EndOfMonth/>
            <FirstDate/>
            <LastDate/>
          </Rules>
        </ScheduleData>
      </LegData>
      <!-- Index constituents notional sum times tranche
	   width must match the premium leg notional
      -->
      <Basket>
        <Name>
          <IssuerId>CPTY_1</IssuerId>
          <CreditCurveId>CREDIT_CURVE_1</CreditCurveId>
          <Notional>100000.0</Notional>
